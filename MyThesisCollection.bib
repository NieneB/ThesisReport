@article{Clarke2011,
abstract = {a b s t r a c t Increasing attention is being paid to the importance of built environment characteristics for participa-tion, especially among people with various levels of impairment or activity limitations. The purpose of this research was to examine the role of specific characteristics in the urban environment as they interact with underlying impairments and activity limitations to either promote or hinder participation in life situations. Using data from the Chicago Community Adult Health Study (2001e2003) in the United States, we used logistic regression to examine the effect of built environment characteristics on three indicators of participation (interpersonal interaction, obtaining preventive health care, and voting) among adults age 45þ (N ¼ 1225). We examined effects across two levels of spatial scale: the census tract and block group. One in five adults reported difficulty walking 2e3 blocks unaided, but their odds of engaging in regular interpersonal interaction was 45\% higher when they lived in areas with higher residential security. For the thirty-six percent of adults who reported visual impairment, and the odds of obtaining preventive health care were over 20\% lower when living in an area with heavy traffic. Residing in an area with a high proportion of streets in poor condition was associated with 60\% lower odds of voting among those with underlying difficulty with mobility activities. Results varied across levels of spatial scale. Simple changes in urban built environments may facilitate the full participation of all persons in society.},
author = {Clarke, Philippa J and Ailshire, Jennifer A and Nieuwenhuijsen, Els R and {De Kleijn E De Vrankrijker}, Marijke W},
doi = {10.1016/j.socscimed.2011.03.025},
file = {:C$\backslash$:/Users/boeij004/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Clarke et al. - 2011 - Participation among adults with disability The role of the urban environment.pdf:pdf},
journal = {Social Science \& Medicine},
keywords = {Aging,Built environment,Disability,Interpersonal interaction,USA},
pages = {1674--1684},
title = {{Participation among adults with disability: The role of the urban environment}},
volume = {72},
year = {2011}
}
